@page "/DialogProgressBar"

<div class="row">
    <div class="col-md-12">
        <h3>@Tittle</h3>
        <RadzenProgressBar Value="100" ShowValue="false" Mode="ProgressBarMode.Indeterminate" Style="margin-bottom: 20px" />
    </div>
</div>

@code {
    [Parameter]
    public string Tittle { get; set; }
}